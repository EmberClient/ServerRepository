{
  "type": "object",
  "properties": {
    "name": {
      "type": "string",
      "maxLength": 24
    },
    "hosts": {
      "type": "array",
      "items": {
        "type": "string",
        "format": "hostname"
      }
    },
    "versions": {
      "type": "array",
      "items": {
        "type": "string",
        "enum": ["1.7", "1.8", "1.12", "1.19"]
      }
    },
    "primaryColour": {
      "type": "string",
      "format": "regex",
      "pattern": "^#[A-Fa-f0-9]{6}$"
    },
    "type": {
      "type": "array",
      "items": {
        "type": "string",
        "enum": [
          "Vanilla",
          "Minigames",
          "Skyblock",
          "Survival",
          "Creative",
          "PvP",
          "Prison",
          "Factions"
        ]
      }
    }
  }
}
